{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "plugins": [
    "node"
  ],
  "extends": [
    "plugin:prettier/recommended",
    "eslint:recommended",
    "plugin:node/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "warnOnUnsupportedTypeScriptVersion": false
  },
  "rules": {
    "prettier/prettier": "error",
    "node/no-deprecated-api": 0,
    "no-console": 0
  }
}
