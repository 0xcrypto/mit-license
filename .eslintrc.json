{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "plugins": ["node"],
  "extends": ["eslint:recommended", "plugin:node/recommended"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "warnOnUnsupportedTypeScriptVersion": false
  },
  "rules": {
    "node/no-deprecated-api": 0,
    "no-console": 0
  }
}
